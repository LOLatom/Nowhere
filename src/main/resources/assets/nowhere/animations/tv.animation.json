{
	"format_version": "1.8.0",
	"animations": {
		"animation.tv.turnon": {
			"loop": true
		},
		"animation.tv.off": {
			"loop": true
		},
		"animation.tv.on": {
			"loop": true,
			"bones": {
				"Locust": {
					"position": {
						"vector": [0, 0, -1],
						"easing": "linear"
					}
				},
				"Torso": {
					"position": {
						"vector": [0, "math.sin(query.anim_time * 150) * 0.02", 0],
						"easing": "linear"
					}
				},
				"Head": {
					"position": {
						"vector": [0, "math.cos(query.anim_time * 150) * 0.05", 0],
						"easing": "linear"
					}
				},
				"Jaw": {
					"position": {
						"vector": [0, "math.sin(query.anim_time * 150) * 0.05", 0],
						"easing": "linear"
					}
				}
			}
		}
	},
	"geckolib_format_version": 2
}